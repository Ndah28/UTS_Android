<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="polinema.ac.id.androiduistarter">

    <application
        android:allowBackup="true"
        android:icon="@drawable/cepat"
        android:label="@string/app_name"
        android:roundIcon="@drawable/cepat"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".fragments.activities.ForgotPasswordActivity" />
        <activity android:name=".fragments.activities.LoginActivity" />
        <activity android:name=".fragments.activities.WelcomeActivity" />
        <activity android:name=".fragments.activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".fragments.activities.MainActivity" />

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@menu/navigation" />
    </application>

</manifest>